; vim:ft=lisp

val nil ()

func list (args)

func foreach (
    (if (cadr-args)
        (quote (nil
            ((caar-args) (caadar-args))
            (foreach (caar-args) (cdadar-args))
        ))
        nil
    )
    (args)
)

func map (
    (if (cadr-args)
        (quote (cons
            ((caar-args) (caadar-args))
            (map (caar-args) (cdadar-args))
        ))
        nil
    )
    (args)
)

func sel-says (str-concat "sed lisp says: " (car-args))

val main (foreach print
    (map sel-says
        (list
            "Check it out!"
            "Now with more recursion"
        )
    )
)
